{
    "name": "sericode/laravel-extensions",
    "description": "The \"sericode/laravel-extensions\" package provides some helpful extensions for Laravel Framework.",
    "keywords": [
        "xuanquynh", "laravel", "laravel-extensions"
    ],
    "type": "library",
    "homepage": "https://github.com/sericode/laravel-extensions",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sericode/laravel-extensions/issues",
        "source": "https://github.com/sericode/laravel-extensions"
    },
    "authors": [
        {
            "name": "Nguyễn Xuân Quỳnh",
            "email": "nguyen.xuan.quynh@framgia.com"
        }
    ],
    "require": {
        "illuminate/console": ">=5.6 <5.8",
        "illuminate/contracts": ">=5.6 <5.8",
        "illuminate/database": ">=5.6 <5.8",
        "illuminate/http": ">=5.6 <5.8",
        "illuminate/support": ">=5.6 <5.8"
    },
    "replace": {
        "sericode/laravel-console": "self.version",
        "sericode/laravel-contracts": "self.version",
        "sericode/laravel-repositories": "self.version",
        "sericode/laravel-support": "self.version"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sericode\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sericode\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
